package cn.vetech.vedsb.jp.entity.jptpsz;


import java.util.Date;

import javax.persistence.GeneratedValue;
import javax.persistence.Id;
import javax.persistence.Table;

import org.vetech.core.modules.mybatis.page.AbstractPageEntity;


@Table(name="JP_TPHKSJ_SZ")
public class JpTpsz extends AbstractPageEntity{
	/**
	 * 
	 */
	private static final long serialVersionUID = -1428190045494361400L;

	@Id
	@GeneratedValue(generator="no")
	private String id;
	
	private String hkgs;  	//航空公司
	
	private String pzlx;  	//票证类型
	
	private String sfzytp;  //是否自愿退票
	
	private String yjhksc;  //预计回款天数
	
	private String shbh;  	//商户编号
	
	private String cjyh;    //创建人
	
	private String xgyh;	//修改人
	
	private Date cjsj;		//创建时间
	
	private Date xgsj;		//修改时间
	
	


	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getHkgs() {
		return hkgs;
	}

	public void setHkgs(String hkgs) {
		this.hkgs = hkgs;
	}

	public String getPzlx() {
		return pzlx;
	}

	public void setPzlx(String pzlx) {
		this.pzlx = pzlx;
	}

	public String getSfzytp() {
		return sfzytp;
	}

	public void setSfzytp(String sfzytp) {
		this.sfzytp = sfzytp;
	}

	public String getYjhksc() {
		return yjhksc;
	}

	public void setYjhksc(String yjhksc) {
		this.yjhksc = yjhksc;
	}

	public String getShbh() {
		return shbh;
	}

	public void setShbh(String shbh) {
		this.shbh = shbh;
	}

	public String getCjyh() {
		return cjyh;
	}

	public void setCjyh(String cjyh) {
		this.cjyh = cjyh;
	}

	public String getXgyh() {
		return xgyh;
	}

	public void setXgyh(String xgyh) {
		this.xgyh = xgyh;
	}

	public Date getCjsj() {
		return cjsj;
	}

	public void setCjsj(Date cjsj) {
		this.cjsj = cjsj;
	}

	public Date getXgsj() {
		return xgsj;
	}
	
	public void setXgsj(Date xgsj) {
		this.xgsj = xgsj;
	}	
}
	
	
 