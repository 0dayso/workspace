package cn.vetech.vedsb.jp.entity.jpgqgl;

public enum JpGqdStepEnum {
	/**
	 * TKNE指令
	 */
	TKNE(1, "TKNE指令"), 
	/**
	 * TRD:X指令
	 */
	TRDX(2, "TRD:X指令"), 
	/**
	 * 预订新PNR
	 */
	YDPNR(3, "预订新PNR"),
	/**
	 * XE原PNR
	 */
	XEPNR(4, "XE原PNR"),
	/**
	 * XE原航段
	 */
	XEHD(5, "XE原航段"), 
	/**
	 * 预订新航段
	 */
	YDHD(6, "预订新航段"), 
	/**
	 * 分离原PNR
	 */
	SP(7, "分离原PNR"), 
	/**
	 * XE分离新PNR
	 */
	XESPPNR(8, "XE分离新PNR");

	/**
	 * 步骤ID
	 */
	private int id;

	/**
	 * 步骤名称
	 */
	private String name;

	private JpGqdStepEnum(int id, String name) {
		this.id = id;
		this.name = name;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
}
