package cn.vetech.vedsb.platpolicy.jzcp.b2b.client.response;

import java.io.Serializable;

public class StateInfo implements Serializable{
	/**
	 * 
	 */
	private static final long serialVersionUID = -6998586618377356752L;

	private String userid;// 当前操作员ID

	private String outOrderNo;// 外部订单编号
	private String airwaysOrderNo;// 官网订单编号

	private String airways;// 航空公司
	private String airwaysAccount; // 航司账号

	private String pnr;// pnr编码
	private String hkgspnr;// 大编码

	private boolean isisloginok;// 是否登录
	private String stateenum;// 当前状态
	private int stateenumordinal;// 当前状态 顺序
	private int statecount;//请求的次数

	private String payhand;

	public String getUserid() {
		return userid;
	}

	public void setUserid(String userid) {
		this.userid = userid;
	}

	public String getOutOrderNo() {
		return outOrderNo;
	}

	public void setOutOrderNo(String outOrderNo) {
		this.outOrderNo = outOrderNo;
	}

	public String getAirwaysOrderNo() {
		return airwaysOrderNo;
	}

	public void setAirwaysOrderNo(String airwaysOrderNo) {
		this.airwaysOrderNo = airwaysOrderNo;
	}

	public String getAirways() {
		return airways;
	}

	public void setAirways(String airways) {
		this.airways = airways;
	}

	public String getAirwaysAccount() {
		return airwaysAccount;
	}

	public void setAirwaysAccount(String airwaysAccount) {
		this.airwaysAccount = airwaysAccount;
	}

	public String getPnr() {
		return pnr;
	}

	public void setPnr(String pnr) {
		this.pnr = pnr;
	}

	public String getHkgspnr() {
		return hkgspnr;
	}

	public void setHkgspnr(String hkgspnr) {
		this.hkgspnr = hkgspnr;
	}

	public boolean isIsisloginok() {
		return isisloginok;
	}

	public void setIsisloginok(boolean isisloginok) {
		this.isisloginok = isisloginok;
	}

	public String getStateenum() {
		return stateenum;
	}

	public void setStateenum(String stateenum) {
		this.stateenum = stateenum;
	}

	public int getStateenumordinal() {
		return stateenumordinal;
	}

	public void setStateenumordinal(int stateenumordinal) {
		this.stateenumordinal = stateenumordinal;
	}

	public String getPayhand() {
		return payhand;
	}

	public void setPayhand(String payhand) {
		this.payhand = payhand;
	}

	public int getStatecount() {
		return statecount;
	}

	public void setStatecount(int statecount) {
		this.statecount = statecount;
	}
}
