package cn.vetech.vedsb.platpolicy.cps.response.ticket;

/**
 * 获取实时政策接口输出--精准营销服务类产品Bean
 * 
 * @author vetech
 */
public class MarketSerBean extends PolicyBean {
    
    private String serviceId;// 匹配精准营销营销产品--服务类产品编号
    
    private String serviceType;// 匹配精准营销营销产品--服务类产品类型 2贵宾厅 3接送机 4优惠停车
    
    private String serviceTypeNote;// 匹配精准营销营销产品--服务类产品类型说明 2贵宾厅 3接送机 4优惠停车
    
    private String serviceNote;// 匹配精准营销营销产品--服务类产品说明
    
    private String icon;//图标URL地址，例如：http://192.168.1.69:10006/images/xx.gif
    
    public String getServiceId() {
        return this.serviceId;
    }
    
    public void setServiceId(String serviceId) {
        this.serviceId = serviceId;
    }
    
    public String getServiceType() {
        return this.serviceType;
    }
    
    public void setServiceType(String serviceType) {
        this.serviceType = serviceType;
    }
    
    public String getServiceTypeNote() {
        return this.serviceTypeNote;
    }
    
    public void setServiceTypeNote(String serviceTypeNote) {
        this.serviceTypeNote = serviceTypeNote;
    }
    
    public String getServiceNote() {
        return this.serviceNote;
    }
    
    public void setServiceNote(String serviceNote) {
        this.serviceNote = serviceNote;
    }

    
    public String getIcon() {
        return this.icon;
    }

    
    public void setIcon(String icon) {
        this.icon = icon;
    }
    
}
