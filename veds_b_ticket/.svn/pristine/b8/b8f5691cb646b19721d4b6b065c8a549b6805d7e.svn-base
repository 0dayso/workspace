package cn.vetech.vedsb.platpolicy.cpslink;

import cn.vetech.vedsb.common.entity.Shyhb;

public class PlatSearchParam {
	private String ddbh;
	private Shyhb yhb;
	//0返回普通政策1返回特殊政策 3是都返回(8000翼用到)
	private String isSpecial;
	private String platcodes;
	public String getDdbh() {
		return ddbh;
	}
	public void setDdbh(String ddbh) {
		this.ddbh = ddbh;
	}
	public Shyhb getYhb() {
		return yhb;
	}
	public void setYhb(Shyhb yhb) {
		this.yhb = yhb;
	}
	public String getIsSpecial() {
		return isSpecial;
	}
	public void setIsSpecial(String isSpecial) {
		this.isSpecial = isSpecial;
	}
	public String getPlatcodes() {
		return platcodes;
	}
	public void setPlatcodes(String platcodes) {
		this.platcodes = platcodes;
	}
}
