package org.vetech.core.modules.web.select;


public class SelectNode {
	private String value="";
	
	private String label="";
	
	private String mc="";
	
	private String pyszm="";
	
	private String pyqp="";
	
	private Object attributes;

	public SelectNode(){
		
	}
	public SelectNode(String value, String label) {
		this.value = value;
		this.label = label;
		this.attributes = new Object();
	}
	public SelectNode(String value, String label,String mc,String pyszm,String pyqp) {
		this.value = value;
		this.label = label;
		this.mc = mc;
		this.pyszm = pyszm;
		this.pyqp = pyqp;
		this.attributes = new Object();
	}
	public SelectNode(String value, String label,String mc,String pyszm) {
		this.value = value;
		this.label = label;
		this.mc = mc;
		this.pyszm = pyszm;
		this.attributes = new Object();
	}
	/**
	 * 需要其他属性时，attributes=null
	 * @param bh
	 * @param zwmc
	 * @param pyszm
	 * @param attributes
	 */
	public SelectNode(String value, String label,Object attributes) {
		this.value = value;
		this.label = label;
		this.attributes = attributes;
	}
	public String getValue() {
		return value;
	}
	public void setValue(String value) {
		this.value = value;
	}
	public String getLabel() {
		return label;
	}
	public void setLabel(String label) {
		this.label = label;
	}
	public Object getAttributes() {
		return attributes;
	}
	public void setAttributes(Object attributes) {
		this.attributes = attributes;
	}
	public String getMc() {
		return mc;
	}
	public void setMc(String mc) {
		this.mc = mc;
	}
	public String getPyszm() {
		return pyszm;
	}
	public void setPyszm(String pyszm) {
		this.pyszm = pyszm;
	}
	public String getPyqp() {
		return pyqp;
	}
	public void setPyqp(String pyqp) {
		this.pyqp = pyqp;
	}

	
	
}
