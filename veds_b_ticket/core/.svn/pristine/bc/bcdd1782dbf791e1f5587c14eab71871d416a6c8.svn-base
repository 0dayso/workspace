package org.vetech.core.business.pid.api.avh;

import org.vetech.core.business.pid.api.Param;

/**
 * avh接口入参
 * @author 章磊
 *
 */
public class AvhParam extends Param{
	private String cfrq;	//出发日期，格式是yyyy-mm-dd，必填
	private String cfcs;  	//出发城市,必填
	private String ddcs;	//到达城市,必填
	private String hkgs;	//航空公司,不必填
	private String hc;	//航程,不必填
	private String hbh;	//航班号,不必填
	private String sfzd;	//是否直达,1直达，0非直达，必填
	
	public String getDdcs() {
		return ddcs;
	}
	public void setDdcs(String ddcs) {
		this.ddcs = ddcs;
	}
	public String getCfrq() {
		return cfrq;
	}
	public void setCfrq(String cfrq) {
		this.cfrq = cfrq;
	}
	public String getCfcs() {
		return cfcs;
	}
	public void setCfcs(String cfcs) {
		this.cfcs = cfcs;
	}
	public String getHkgs() {
		return hkgs;
	}
	public void setHkgs(String hkgs) {
		this.hkgs = hkgs;
	}
	public String getHc() {
		return hc;
	}
	public void setHc(String hc) {
		this.hc = hc;
	}
	public String getHbh() {
		return hbh;
	}
	public void setHbh(String hbh) {
		this.hbh = hbh;
	}
	public String getSfzd() {
		return sfzd;
	}
	public void setSfzd(String sfzd) {
		this.sfzd = sfzd;
	}
	
}
